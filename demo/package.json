{
  "private": true,
  "name": "demo",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "predeploy": "yarn export",
    "deploy": "gh-pages -t -d dist",
    "preexport": "rimraf dist && NEXT_PUBLIC_BASE_PATH=/google-login yarn build",
    "export": "NEXT_PUBLIC_BASE_PATH=/google-login next export -o dist && touch dist/.nojekyll",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write"
  },
  "dependencies": {
    "@cs124/pingpongws-client": "2023.1.0",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reconnecting-websocket": "4.4.0",
    "runtypes": "6.6.0"
  },
  "devDependencies": {
    "@types/react": "18.0.26",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "gh-pages": "4.0.0",
    "prettier": "2.8.2",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "3.2.1",
    "sass": "1.57.1",
    "typescript": "4.9.4"
  }
}
