{
  "private": true,
  "name": "demo",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "predeploy": "yarn export",
    "deploy": "gh-pages -t -d dist",
    "preexport": "rimraf dist && NEXT_PUBLIC_BASE_PATH=/google-login yarn build",
    "export": "NEXT_PUBLIC_BASE_PATH=/google-login next export -o dist && touch dist/.nojekyll",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write"
  },
  "dependencies": {
    "@cs124/pingpongws-client": "2021.7.0",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reconnecting-websocket": "^4.4.0",
    "runtypes": "^6.3.1"
  },
  "devDependencies": {
    "@types/react": "17.0.14",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "gh-pages": "^3.2.3",
    "prettier": "2.3.2",
    "prettier-package-json": "2.6.0",
    "prettier-plugin-organize-imports": "2.2.0",
    "sass": "1.35.2",
    "typescript": "4.3.5"
  }
}
